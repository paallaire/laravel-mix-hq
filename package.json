    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.4",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "jquery": "^3.5.0"
    }
